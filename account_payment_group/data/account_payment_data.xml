<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Payment sequences -->
        <record id="sequence_payment_group" model="ir.sequence">
            <field name="name">Payments group sequence</field>
            <field name="code">account.payment.group</field>
            <field name="prefix">COMP.PAGO/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_payment_group_receiver" model="ir.sequence">
            <field name="name">Payments group sequence receiver</field>
            <field name="code">account.payment.group.receiver</field>
            <field name="prefix">COMP.COB/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
